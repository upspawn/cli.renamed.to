{
  "name": "@renamed-to/cli",
  "version": "1.0.1",
  "description": "CLI tool for AI-powered file renaming, PDF extraction, and document splitting",
  "private": false,
  "type": "module",
  "license": "MIT",
  "author": "Renamed.to",
  "homepage": "https://renamed.to",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/upspawn/cli.renamed.to.git"
  },
  "bugs": {
    "url": "https://github.com/upspawn/cli.renamed.to/issues"
  },
  "keywords": [
    "cli",
    "rename",
    "pdf",
    "ai",
    "ocr",
    "extract",
    "split",
    "document"
  ],
  "engines": {
    "node": ">=20.0.0"
  },
  "bin": {
    "renamed": "dist/index.js"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsup",
    "build:binaries": "bun run scripts/build-binaries.ts",
    "dev": "tsup --watch",
    "lint": "eslint",
    "test": "vitest run",
    "test:e2e": "npx tsx scripts/e2e-test.ts",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@opentui/core": "^0.1.69",
    "chalk": "^5.6.2",
    "chokidar": "^5.0.0",
    "cli-table3": "^0.6.5",
    "commander": "^14.0.2",
    "conf": "^15.0.2",
    "open": "^11.0.0",
    "ora": "^9.0.0",
    "prompts": "^2.4.2",
    "yaml": "^2.8.2",
    "zod": "^4.3.5"
  },
  "devDependencies": {
    "@types/cli-table": "^0.3.4",
    "@types/node": "^25.0.3",
    "@types/prompts": "^2.4.9",
    "@vitest/coverage-v8": "4.0.16",
    "eslint": "^9.39.2",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.52.0",
    "vitest": "^4.0.16"
  }
}
